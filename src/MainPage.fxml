   <children>
      <ImageView fitHeight="184.0" fitWidth="562.0" layoutX="21.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@download.png" />
         </image>
      </ImageView>
      <Button fx:id="studentButton" layoutX="21.0" layoutY="240.0" mnemonicParsing="false" onAction="#openStudent" text="Student" />
      <Button fx:id="courses" layoutX="427.0" layoutY="240.0" mnemonicParsing="false" onAction="#openCourses" text="Courses" />
   </children>
</AnchorPane>
