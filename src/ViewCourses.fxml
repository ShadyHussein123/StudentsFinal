<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewCoursesController">
   <children>
      <TableView fx:id="coursesTableView" layoutY="3.0" prefHeight="351.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="courseNameColumn" prefWidth="124.0" text="Course Name" />
          <TableColumn fx:id="courseIDColumn" prefWidth="134.0" text="Course ID" />
            <TableColumn fx:id="profColumn" prefWidth="157.0" text="Professor" />
            <TableColumn fx:id="majorColumn" prefWidth="182.0" text="Major" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="14.0" layoutY="359.0" mnemonicParsing="false" onAction="#backToCourses" text="Back" />
   </children>
</AnchorPane>
