<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="420.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SearchStudentController">
   <children>
      <TableView fx:id="tableView" layoutY="3.0" prefHeight="383.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="180.0" text="Student Name" />
          <TableColumn fx:id="studentIDColumn" prefWidth="201.0" text="Student ID" />
            <TableColumn fx:id="studentMajorColumn" prefWidth="218.0" text="Major" />
        </columns>
      </TableView>
      <Button fx:id="backToStudent" layoutX="14.0" layoutY="386.0" mnemonicParsing="false" onAction="#backToStudent" text="Back" />
      <Button fx:id="deleteButton" layoutX="527.0" layoutY="386.0" mnemonicParsing="false" onAction="#deleteStudent" text="Delete" />
   </children>
</AnchorPane>
