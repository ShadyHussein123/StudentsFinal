<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CourseToStudentController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Choose Course: " />
      <ChoiceBox fx:id="courseMajorBox" layoutX="112.0" layoutY="10.0" prefWidth="150.0" />
      <TableView fx:id="tableview" layoutX="12.0" layoutY="68.0" prefHeight="328.0" prefWidth="582.0">
