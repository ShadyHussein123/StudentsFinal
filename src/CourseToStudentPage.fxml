      </TableView>
      <Label layoutX="283.0" layoutY="15.0" text="Student: " />
      <ChoiceBox fx:id="studentMajorBox" layoutX="342.0" layoutY="11.0" prefWidth="150.0" />
      <Button fx:id="addButton" layoutX="532.0" layoutY="9.0" mnemonicParsing="false" onAction="#addCourseToStudent" text="Add " />
