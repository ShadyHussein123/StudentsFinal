          <TableColumn fx:id="studentNameColumn" prefWidth="195.0" text="Student Name" />
          <TableColumn fx:id="studentIDColumn" prefWidth="205.0" text="Student ID" />
            <TableColumn fx:id="studentMajorColumn" prefWidth="181.0" text="Student Major" />
